<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.person.dao.PersonDAO">
	<select id="selectPersonList" resultType="PersonVO">
		SELECT ID,NAME,GENDER,AGE,ADDRESS FROM PERSON
	</select>
	
	<select id="selectPerson" resultType="PersonVO">
		SELECT ID,NAME,GENDER,AGE,ADDRESS FROM PERSON WHERE ID=#{id}
	</select>
</mapper>